{
  "name": "@pkg/client",
  "version": "0.1.0",
  "license": "ISC",
  "private": true,
  "scripts": {
    "typegen": "apollo client:codegen --target=typescript --globalTypesFile=src/__generated__/globalTypes.ts && rm -rf __generated__",
    "build": "react-app-rewired build",
    "dev": "BROWSER=false react-app-rewired start",
    "repl": "ts-node-dev --respawn src/repl",
    "analyze": "ANALYZE=1 npm run build",
    "test": "react-app-rewired test",
    "e2e": "cd e2e && jest"
  },
  "browserslist": {
    "production": [
      ">5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "graphql": "^15.3.0",
    "history": "^5.0.0",
    "preact": "^10.4.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.3.1",
    "react-metatags-hook": "^1.3.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-use": "^15.3.3",
    "react-useportal": "^1.0.13"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/expect-puppeteer": "^4.4.3",
    "@types/jest": "^24.0.0",
    "@types/jest-environment-puppeteer": "^4.4.0",
    "@types/node": "^12.0.0",
    "@types/puppeteer": "^3.0.2",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "apollo": "^2.31.0",
    "eslint": "^6.6.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": ">=10",
    "prettier": "^2.0.5",
    "puppeteer": "^5.3.1",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "^3.4.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "engines": {
    "node": ">=12 <15"
  }
}
