{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es2016",
    "module": "commonjs",
    "lib": ["dom","esnext"],
    "rootDir": "src",
    "strict": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true
  },
  "include": ["src"],
  "exclude": ["prisma/**/*", "node_modules"]
}
